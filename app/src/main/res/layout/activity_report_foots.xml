<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/bg_gray" >

    <include
        android:id="@+id/title"
        layout="@layout/head_title" />

    <ScrollView
        android:id="@+id/scrollView1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/title"
        android:background="@color/register_gray_light"
        android:scrollbars="none" >

        <LinearLayout
            android:id="@+id/ll_report_foot"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical" >

            <TextView
                android:id="@+id/tv_report_foot_date"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@android:color/darker_gray"
                android:padding="@dimen/y6"
                android:text="报告日期："
                android:textColor="@android:color/white" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal" >

                <View
                    android:layout_width="0dp"
                    android:layout_height="1dp"
                    android:layout_gravity="center_vertical"
                    android:layout_weight="1"
                    android:background="#d9d9d9" />

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:padding="@dimen/y6"
                    android:text="压力图谱"
                    android:textSize="16sp" />

                <View
                    android:layout_width="0dp"
                    android:layout_height="1dp"
                    android:layout_gravity="center_vertical"
                    android:layout_weight="1"
                    android:background="#d9d9d9" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/ll_report_foot_print"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center" >

                <View
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="0.2" />

                <FrameLayout
                    android:id="@+id/fl_left"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:orientation="vertical" >

                    <ImageView
                        android:id="@+id/iv_leftshoe"
                        android:layout_width="@dimen/y168"
                        android:layout_height="@dimen/y543"
                        android:layout_gravity="center"
                        android:scaleType="fitCenter"
                        android:src="@drawable/bind_stress_left_shoe" />

                    <cn.com.amome.amomeshoes.widget.CircleView
                        android:id="@+id/canves_left"
                        android:layout_width="@dimen/y168"
                        android:layout_height="@dimen/y543"
                        android:layout_gravity="center" />
                </FrameLayout>

                <FrameLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:orientation="vertical" >

                    <ImageView
                        android:id="@+id/iv_rightshoe"
                        android:layout_width="@dimen/y168"
                        android:layout_height="@dimen/y543"
                        android:layout_gravity="center"
                        android:scaleType="fitCenter"
                        android:src="@drawable/bind_stress_right_shoe" />

                    <cn.com.amome.amomeshoes.widget.CircleView
                        android:id="@+id/canves_right"
                        android:layout_width="@dimen/y168"
                        android:layout_height="@dimen/y543"
                        android:layout_gravity="center" />
                </FrameLayout>

                <View
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="0.2" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal" >

                <View
                    android:layout_width="0dp"
                    android:layout_height="1dp"
                    android:layout_gravity="center_vertical"
                    android:layout_weight="1"
                    android:background="#d9d9d9" />

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:padding="@dimen/y6"
                    android:text="足形评估"
                    android:textSize="16sp" />

                <View
                    android:layout_width="0dp"
                    android:layout_height="1dp"
                    android:layout_gravity="center_vertical"
                    android:layout_weight="1"
                    android:background="#d9d9d9" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/ll_report_foot_shape"
                android:layout_width="match_parent"
                android:layout_height="@dimen/y272"
                android:orientation="horizontal" >

                <View
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="0.3" />

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:orientation="vertical" >

                    <ImageView
                        android:id="@+id/iv_instep"
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_gravity="center"
                        android:layout_weight="2"
                        android:scaleType="fitCenter" />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="1"
                        android:orientation="horizontal" >

                        <TextView
                            android:id="@+id/tv_instep_result"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:gravity="center"
                            android:textSize="14sp" />
                    </LinearLayout>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:orientation="vertical" >

                    <ImageView
                        android:id="@+id/iv_spin"
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_gravity="center"
                        android:layout_weight="2"
                        android:scaleType="fitCenter" />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="1"
                        android:orientation="horizontal" >

                        <TextView
                            android:id="@+id/tv_spin_result"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:gravity="center"
                            android:textSize="14sp" >
                        </TextView>
                    </LinearLayout>
                </LinearLayout>

                <View
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="0.3" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/ll_report_foot_dise"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:visibility="gone" >

                <View
                    android:layout_width="0dp"
                    android:layout_height="1dp"
                    android:layout_gravity="center_vertical"
                    android:layout_weight="1"
                    android:background="#d9d9d9" />

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:padding="@dimen/y6"
                    android:text="足疾评估"
                    android:textSize="16sp" />

                <View
                    android:layout_width="0dp"
                    android:layout_height="1dp"
                    android:layout_gravity="center_vertical"
                    android:layout_weight="1"
                    android:background="#d9d9d9" />
            </LinearLayout>

            <cn.com.amome.amomeshoes.widget.MyGridView
                android:id="@+id/gv_other"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/y33"
                android:layout_marginRight="@dimen/y33"
                android:cacheColorHint="@android:color/transparent"
                android:horizontalSpacing="@dimen/y10"
                android:listSelector="@android:color/transparent"
                android:numColumns="4"
                android:scrollbars="none"
                android:verticalSpacing="0dp" >
            </cn.com.amome.amomeshoes.widget.MyGridView>

            <LinearLayout
                android:id="@+id/ll_title_foot_report"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:visibility="gone" >

                <View
                    android:layout_width="0dp"
                    android:layout_height="1dp"
                    android:layout_gravity="center_vertical"
                    android:layout_weight="1"
                    android:background="#d9d9d9" />

                <TextView
                    android:id="@+id/tv_title_foot_report"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:padding="@dimen/y6"
                    android:text="足部报告"
                    android:textSize="16sp" />

                <View
                    android:layout_width="0dp"
                    android:layout_height="1dp"
                    android:layout_gravity="center_vertical"
                    android:layout_weight="1"
                    android:background="#d9d9d9" />
            </LinearLayout>

            <TextView
                android:id="@+id/tv_instep_name"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:paddingLeft="@dimen/y21"
                android:paddingRight="@dimen/y21"
                android:text=""
                android:textSize="14sp"
                android:visibility="gone" />

            <TextView
                android:id="@+id/tv_instep_tip"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/y42"
                android:paddingLeft="@dimen/y21"
                android:paddingRight="@dimen/y21"
                android:text=""
                android:textSize="13sp"
                android:visibility="gone" />

            <TextView
                android:id="@+id/tv_spin_name"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:paddingLeft="@dimen/y21"
                android:paddingRight="@dimen/y21"
                android:text=""
                android:textSize="14sp"
                android:visibility="gone" />

            <TextView
                android:id="@+id/tv_spin_tip"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/y42"
                android:paddingLeft="@dimen/y21"
                android:paddingRight="@dimen/y21"
                android:text=""
                android:textSize="13sp"
                android:visibility="gone" />



            <!--2017/9/28新增足部管家部分-->
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:visibility="visible">

                <View
                    android:layout_width="0dp"
                    android:layout_height="1dp"
                    android:layout_gravity="center_vertical"
                    android:layout_weight="1"
                    android:background="#d9d9d9" />

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:padding="@dimen/y6"
                    android:text="足部管家"
                    android:textSize="16sp" />

                <View
                    android:layout_width="0dp"
                    android:layout_height="1dp"
                    android:layout_gravity="center_vertical"
                    android:layout_weight="1"
                    android:background="#d9d9d9" />
            </LinearLayout>
            <!--添加足部管家中有解决方案&&用户有的疾病-->
            <!--这下面的打包不显示-->
            <android.support.v7.widget.RecyclerView
                android:id="@+id/re_foot"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:visibility="visible">
            </android.support.v7.widget.RecyclerView>
            <Button
                android:id="@+id/btn_add"
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:text="+  一键添加"
                android:textSize="20sp"
                android:background="#3ED6BB"
                android:textColor="#FFFFFF"
                android:visibility="visible"/>
        </LinearLayout>
    </ScrollView>

    <RelativeLayout
        android:id="@+id/ll_report_null"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/title"
        android:background="@color/bg_gray" >

        <TextView
            android:id="@+id/tv_no_data_tip"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/y30"
            android:gravity="center"
            android:padding="@dimen/y10"
            android:text="魔秘还没有收集到您的足部信息，\n做完“蹲一蹲”再来看看吧"
            android:visibility="gone" />
    </RelativeLayout>

</RelativeLayout>